  body

    .page
      //- Main Navbar
      header.header
        nav.navbar
          //- Search Box
          .container-fluid
            .navbar-holder.d-flex.align-items-center.justify-content-between
              //- Navbar Header
              .navbar-header
                //- Navbar Brand
                a.navbar-brand.d-none.d-sm-inline-block(href="/")
                  .brand-text.d-none.d-lg-inline-block
                    span SUB
                    strong Zone3
                  .brand-text.d-none.d-sm-inline-block.d-lg-none
                    strong BD
                //- Toggle Button
                a#toggle-btn.menu-btn.active(href="#")
                  span
                  span
                  span
              //- Navbar Menu
              ul.nav-menu.list-unstyled.d-flex.flex-md-row.align-items-md-center
                //- Logout
                li.nav-item
                  a.nav-link.logout(href="/login")
                    span.d-none.d-sm-inline Logout &nbsp;
                    i.fas.fa-sign-out-alt
      .page-content.d-flex.align-items-stretch
        //- Side Navbar
        nav.side-navbar
          //- Sidebar Header
          .sidebar-header.d-flex.align-items-center
            .title
              h1.h4 Welcome
              p Planer
          //- Sidebar Navidation Menus
          span.heading(style="font-size:14px") หัวข้อ
            ul.list-unstyled
              li(class=(pageNo=='index'? 'active':''))
                a(href="/"  onclick="$(this).loadingStatus()")
                  span(style="font-size: 16px")
                    i.fas.fa-home
                  |   &nbsp; หน้าหลัก
              li(class=(pageNo=='calenEdit'? 'active':''))
                a(href="/calendarEdit" onclick="$(this).loadingStatus()")
                  span(style="font-size: 16px")
                    i.fas.fa-calendar-alt
                  |   &nbsp; จัดการปฏิทิน
              li
                a(href="#exampledropdownDropdown" aria-expanded="false" data-toggle="collapse")
                  span(style="font-size: 16px")
                    i.fas.fa-database
                  |   &nbsp; บริหารจัดการข้อมูลแผนก
                ul#exampledropdownDropdown(class= ((pageNo =='data_z3'||pageNo =='data_z3_eq')? "collapse list-unstyled show" : "collapse list-unstyled"))
                  li(class=(pageNo=='data_z3'? 'active':''))
                    a(href="#") 
                      span(style="font-size: 14px")
                        i.fas.fa-user-edit
                      |   &nbsp; ข้อมูลบุคลากร
                  li(class=(pageNo=='data_z3_eq'? 'active':''))
                    a(href="/equipment" onclick="$(this).loadingStatus()") 
                      span(style="font-size: 14px")
                        i.fas.fa-wrench
                      |   &nbsp; ข้อมูลอุปกรณ์
              li
                a(href="#exampledropdownDropdown2" aria-expanded= "false" data-toggle="collapse")
                  span(style="font-size: 16px")
                    i.fas.fa-file-alt
                  |   &nbsp; รายงาน
                ul#exampledropdownDropdown2(class= ((pageNo =='sta_ser'||pageNo =='report_week')? "collapse list-unstyled show" : "collapse list-unstyled"))
                  li(class=(pageNo=='sta_ser'? 'active':''))
                    a(href="/station_service" onclick="$(this).loadingStatus()") 
                      span(style="font-size: 14px")
                        i.fas.fa-chart-bar
                      |   &nbsp; รายงาน Station Service
                  li(class=(pageNo=='report_week'? 'active':''))
                    a(href="#") 
                      span(style="font-size: 14px")
                        i.fas.fa-file-word
                      |   &nbsp; รายงานประจำสัปดาห์

              li
                a(href="#")
                  span(style="font-size: 16px")
                    i.fas.fa-shopping-cart
                  |   &nbsp; ระบบยืมครุภัณฑ์ pool
         
              li
                a(href="#")
                  span(style="font-size: 16px")
                    i.fas.fa-tachometer-alt
                  |   &nbsp; งานทดสอบ

        
